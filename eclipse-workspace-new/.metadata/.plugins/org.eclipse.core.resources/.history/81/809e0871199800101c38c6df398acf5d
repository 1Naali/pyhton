package com.main;

public class Tick {
	
	private int f = 0;

	public void tick() {
		
		GameMain.map.tick();
		GameMain.cam.tick();
		GameMain.p.tick();
		
		f++;
		if(f >= 60) {
			System.out.println("Py : "+GameMain.p.getY());
			System.out.println("Px : "+GameMain.p.getX());
			System.out.println((GameMain.WIDTH-16)/(GameMain.SCALE));
			f=0;
		}
		
		
	}
}
