package com.main;

public class Tick {
	
	private int f = 0;

	public void tick() {
		
		GameMain.map.tick();
		GameMain.cam.tick();
		GameMain.p.tick();
		
		f++;
		if(f >= 60) {
			System.out.println("Py : "+GameMain.p.getY());
			System.out.println("Px : "+GameMain.p.getX());
			System.out.println("Cy : "+GameMain.cam.getY());
			System.out.println("Cx : "+(GameMain.cam.getX()-GameMain.WIDTH/2));
			f=0;
		}
		
		
	}
}
