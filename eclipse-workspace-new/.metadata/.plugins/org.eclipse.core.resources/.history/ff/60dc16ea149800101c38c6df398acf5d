package com.main;

public class Tick {
	
	private int f = 0;

	public void tick() {
		
		GameMain.map.tick();
		GameMain.cam.tick();
		GameMain.p.tick();
		
		f++;
		if(f >= 30) {
			System.out.println("Py : "+GameMain.p.getY()/GameMain.SCALE);
			System.out.println("Px : "+GameMain.p.getX()/GameMain.SCALE);
			f=0;
		}
		
		
	}
}
